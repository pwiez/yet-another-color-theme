{
    "$schema": "vscode://schemas/color-theme",
    "type": "light",
    "name": "Yet Another Color Theme",
    "colors": {
        // Workbench colors (UI elements)
        "editor.background": "#F5F5F5",
        "editor.foreground": "#333333",
        "editor.lineHighlightBackground": "#E6F2FF",
        "editor.selectionBackground": "#D3E8FF",
        "editorWhitespace.foreground": "#CCCCCC",
        "terminal.background": "#F5F5F5",
        "terminal.foreground": "#333333",
        "terminal.ansiBlack": "#333333",
        "terminal.ansiBlue": "#0078D2",
        "terminal.ansiGreen": "#1EB85A",
        "terminal.ansiYellow": "#FF6100",
        "terminal.ansiRed": "#550080",
        "terminal.ansiCyan": "#00B3B3",
        "terminal.ansiMagenta": "#550080",
        "terminal.ansiWhite": "#F5F5F5",
        "terminal.ansiBrightBlack": "#6A737D",
        "terminal.ansiBrightBlue": "#3399FF",
        "terminal.ansiBrightGreen": "#A8FF60",
        "terminal.ansiBrightYellow": "#FF8000",
        "terminal.ansiBrightRed": "#FF3333",
        "terminal.ansiBrightCyan": "#00FFFF",
        "terminal.ansiBrightMagenta": "#CC00CC",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "sash.hoverBorder": "#c6e1ff",
        "activityBar.background": "#FFFFFF",
        "sideBar.background": "#F8F8F8",
        "editorGroupHeader.tabsBackground": "#FFFFFF",
        "tab.activeBackground": "#F5F5F5",
        "tab.inactiveBackground": "#FFFFFF",
        "statusBar.background": "#696969",
        "panel.background": "#F8F8F8",
        "button.background": "#007bff",
        "list.activeSelectionBackground": "#007bff",
        "list.hoverBackground": "#c6e1ff"
    },
    "tokenColors": [
        // Syntax highlighting TextMate rules
        {
            "scope": "comment",
            "settings": { "foreground": "#6A737D" }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new", "keyword.control.case"],
            "settings": { "foreground": "#0078D2" }
        },
        {
            "scope": ["entity.name.type", "support.class", "entity.other.attribute-name", "meta.preprocessor", "keyword.control.macro", "entity.name.type.class", "entity.name.type.enum", "entity.name.type.struct", "entity.name.type.union", "entity.name.type.typedef", "entity.name.type.trait", "entity.name.type.interface"],
            "settings": { "foreground": "#1EB85A" }
        },
        {
            "scope": ["entity.name.function", "support.function", "meta.function", "entity.name.method"],
            "settings": { "foreground": "#FF6100" }
        },
        {
            "scope": ["string", "punctuation.definition.string", "string.quoted", "string.regexp", "markup.inline.raw"],
            "settings": { "foreground": "#550080" }
        },
        {
            "scope": "constant.numeric",
            "settings": { "foreground": "#00B3B3" }
        },
        {
            "scope": ["variable", "variable.language", "variable.other", "variable.parameter", "variable.declaration"],
            "settings": { "foreground": "#333333" }
        },
        {
            "scope": "keyword.operator",
            "settings": { "foreground": "#000000" }
        },
        {
            "scope": ["entity.other.attribute-name.html", "entity.other.attribute-name.js", "entity.other.attribute-name.jsx", "entity.other.attribute-name.ts", "entity.other.attribute-name.tsx"],
            "settings": { "foreground": "#1EB85A" }
        },
        {
            "scope": "constant.character",
            "settings": { "foreground": "#550080" }
        },
        {
            "scope": ["markup.underline.link", "meta.url", "markup.link"],
            "settings": { "foreground": "#006699" }
        },
        {
            "scope": ["punctuation.accessor", "punctuation.separator"],
            "settings": { "foreground": "#333333" }
        },
        {
            "scope": "support.variable",
            "settings": { "foreground": "#333333" }
        }
    ],
    "semanticHighlighting": true
}